<ng-template #content let-modal>
    <div class="modal-header">
        <h4 class="modal-title">{{msg}}</h4>
        <button type="button" class="close" aria-label="Close" (click)="modal.dismiss('Cross click')">
        <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="modal-body">
        <div class="alert alert-danger" *ngIf="err">{{successmsg}}</div>
        <label>Select Language</label>
        <ng-select style="margin-bottom: 1vh" [(ngModel)]="selectedLg">
        <ng-option *ngFor="let item of lg" [value]="item.id">{{item.name}}</ng-option>
        </ng-select>
        <label>Select Source</label>
        <ng-select [(ngModel)]="selectedType">
        <ng-option *ngFor="let item of type" [value]="item.id">{{item.name}}</ng-option>
        </ng-select><br />
        <div class="custom-file" style="margin-bottom: 3vh" *ngIf="selectedType==1 || selectedType==0">
            <input type="file" [ngClass]="{'is-invalid': fileinvalid}" (change)="handleFileInput($event.target.files,1)"
            class="custom-file-input" id="validatedCustomFile" required>
            <label class="custom-file-label" for="validatedCustomFile" id="lbl">Choose audio file</label>
            <div class="invalid-feedback">{{invalidmsg}}</div>
        </div>
        <div class="input-group" *ngIf="selectedType==4">
            <input type="text" class="form-control" (input)="onSearchChange($event.target.value)"
            [ngClass]="{'is-invalid': exturlinvalid}" [(ngModel)]="mytxturl" placeholder="enter external url">
            <div class="invalid-feedback">
                Enter url
            </div>
            <br /><br /><br />
        </div>
        <div class="custom-file" style="margin-bottom: 3vh">
            <input type="file" [ngClass]="{'is-invalid': fileinvalid2}"
            (change)="handleFileInput($event.target.files,2)" class="custom-file-input" id="validatedCustomFile"
            required>
            <label class="custom-file-label" for="validatedCustomFile" id="lbl2">Choose image file</label>
            <div class="invalid-feedback">{{invalidmsg2}}</div>
        </div>
        <br />
        <label>Select Shabadid</label>
        <ng-select [(ngModel)]="selectedShabad">
        <ng-option *ngFor="let item of shabadlist" [value]="item.id">{{item.id}}</ng-option>
        </ng-select>
        <a (click)="open" href="./search" target="_blank">Find Shabad Id</a>
        <br /><br />
        <div class="input-group">
            <input type="text" class="form-control" id="inputtxt" [ngClass]="{'is-invalid': nameinvalid}"
            [(ngModel)]="mytitle" placeholder="Podcast title">
            <div class="invalid-feedback">
                Enter Podcast title
            </div>
        </div>
        <br />
        <div class="input-group">
            <textarea class="form-control" id="inputtxt" [ngClass]="{'is-invalid': descinvalid}" [(ngModel)]="mydesc"
            placeholder="Podcast Describtion"></textarea>
            <div class="invalid-feedback">
                Enter Podcast description
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-6">
                <label>Select Categories</label>
                <ng-select [multiple]="true" [(ngModel)]="selectedCategory">
                <ng-option *ngFor="let item of categorieslist" [value]="item.id">{{item.name}}</ng-option>
                </ng-select><br />
            </div>
            <div class="col-6">
                <label>Select Sub Categories</label>
                <ng-select [multiple]="true" [(ngModel)]="selectedSubCategory">
                <ng-option *ngFor="let item of subcategorylist" [value]="item.id">{{item.name}}</ng-option>
                </ng-select>
            </div>
        </div>
        <div class="row" style="margin-top:2vh;margin-left:0.5vw">
            <button class="btn btn-primary" (click)="upload(loader)">add</button>
        </div>
    </div>
</ng-template>
<ng-template #loader let-modal>
    <div class="modal-body">
        <div class="d-flex justify-content-center">
            <div class="spinner-border" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <div class="d-flex justify-content-center" *ngIf="showupload" style="margin-top:1vh">
            <span>{{uploaddata}}</span>
        </div>
    </div>
</ng-template>
<ng-template #share let-modal>
    <div class="modal-header">
        <button type="button" class="close" aria-label="Close" (click)="modal.dismiss('Cross click')">
        <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="modal-body" style="display: flex;justify-content:center">
        <share-buttons [include]="['facebook','twitter','print']" [show]="5" [autoSetMeta]="false"
        (opened)="open($event)"></share-buttons>
        <button class="sb-wrapper sb-show-icon" (click)="openModal(export)"
            style="background: #607D8B; font-size: 14px;margin-top: .3125em;border-radius: 4px;color: white">
        <i class="fa fa-link" style="font-size: 14px" aria-hidden="true"></i>
        </button>
    </div>
</ng-template>
<ng-template #export let-modal>
    <div class="modal-header">
        <h4 class="modal-title" id="modal-basic-title">Export</h4>
        <button type="button" class="close" aria-label="Close" (click)="modal.dismiss('Cross click')">
        <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="modal-body">
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox11" value="option1"
                (change)="CheckValue2($event,'Scripture','Gurmukhi-Pad Ched',1)">
            <label class="form-check-label" for="inlineCheckbox11">Gurmukhi-Pad Ched</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox"
                (change)="CheckValue2($event,'ScriptureOriginal','Gurmukhi-Lareevaar',1)" id="inlineCheckbox2"
                value="option2">
            <label class="form-check-label" for="inlineCheckbox2">Gurmukhi-Lareevaar</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox3"
                (change)="CheckValue2($event,'ScriptureRoman','Roman Script',1)" value="option3">
            <label class="form-check-label" for="inlineCheckbox3">Roman Script</label>
        </div>
        <!-- <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox3" (change)="CheckValue($event,'Translation','Translation',1)" value="option3">
            <label class="form-check-label" for="inlineCheckbox3">Translation</label>
            </div> -->
        <div class="h5" style="padding-top:1.5em;padding-bottom:1em">English Translation</div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value=""
                (change)="CheckValue2($event,'ManmohanSinghEnglish','Bhai Manmohan Singh',2)" id="defaultCheck1">
            <label class="form-check-label" for="defaultCheck1">
            Bhai Manmohan Singh
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value=""
                (change)="CheckValue2($event,'SantSinghKhalsaEnglish','Dr. Sant Singh Khalsa',2)" id="defaultCheck2">
            <label class="form-check-label" for="defaultCheck2">
            Dr. Sant Singh Khalsa
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value=""
                (change)="CheckValue2($event,'KhojgurbaaniEnglish','KhojgurbaaniEnglish',2)" id="defaultCheck3">
            <label class="form-check-label" for="defaultCheck3">
            New Translation
            </label>
        </div>
        <div class="h5" style="padding-top:1.5em;padding-bottom:1em">Commentary</div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value=""
                (change)="CheckValue2($event,'ManmohanSinghEnglish','Bhai Manmohan Singh',3)" id="defaultCheck1">
            <label class="form-check-label" for="defaultCheck1">
            Commentary
            </label>
        </div>
        <div class="form-group col-md-4" style="margin-top:1em">
            <select id="inputState" (change)="ChangeValue2($event)" class="form-control">
                <option value="section">Section</option>
                <option value="line">Line</option>
            </select>
        </div>
        <div class="form-group col-md-4">
            <select id="inputState" class="form-control">
                <option>TXT</option>
            </select>
        </div>
        <button class="btn btn-primary" (click)="submit()" style="margin-top:0.5em">Export</button>
    </div>
</ng-template>