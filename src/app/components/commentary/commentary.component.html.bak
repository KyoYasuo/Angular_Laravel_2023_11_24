        <div class="split-container-shabad" style="border: solid 1px #b7b7b7;">
            <div class="column-left">
                <nav class="shabad-pagination" style="display: flex;
                    justify-content: center;
                    align-items: center;
                    background: #1b4154;
                    min-height: 71px;
                    border-radius: 0px;
                    width: 100%;">
                    <div style="background:#1b4154;flex:1;display: flex;
                        align-items: center;border-radius: 4px;min-height:71px;flex-wrap: wrap;">
                        <div style="color:white;padding-left:0.5em">SABAD</div>
                        <div style="flex: 1;
                            display: flex;
                            flex-wrap: wrap;
                            justify-content: center">
                            <div class="circle" (click)="change2()" style="background: #1b4154;cursor: pointer;width: 10px"
                                [class.disabled]="checkdisabled()"> <a style="color:white;font-size: 11px"><i
                                        class="fas fa-sort-down" style="transform: rotate(450deg);"></i>
                                </a>
                            </div>
                            <a class="circle" [routerLink]="['/commentary', pageId, page]"
                                style="cursor: pointer;color:white"
                                *ngFor="let page of shabadPages | slice:startindex:endindex"
                                [ngClass]="{'active2': page === currentShabadPage}">
                                <a style="color:white;font-size: 11px"> {{page}}</a></a>
                            <div class="circle" [class.disabled]="checkdisablednext()" (click)="change3()"
                                style="background: #1b4154;cursor: pointer;width: 10px;margin-right: 5px;"> <a style="color:white;font-size: 11px"> <i
                                        class="fas fa-sort-down fa-rotate-270"></i></a>
                            </div>
                        </div>

                        <div style="flex:1;display: flex;justify-content: center">
                            <!-- <div style="width: 30px;height:30px;cursor: pointer;"  (click)="play2()">
                                <img src="assets/Play.svg" style="width: 32px;
                                    height: 32px;">
                                </div> -->
                            <i class="fa fa-play" aria-hidden="true" id="icon_cir" (click)="play2()"></i>
                            <!-- <div style="width: 30px;height:30px;cursor: pointer;" (click)="openModal(share)">
                                <img src="assets/Share.svg" style="width: 32px;
                                height: 32px;">
                                </div> -->
                            <i class="fa fa-share-alt" aria-hidden="true" id="icon_cir" (click)="openModal(share)"></i>
                            <!-- <div style="width: 30px;height:30px;cursor: pointer;" (click)="paperClip()">
                                <img src="assets/Attach.svg" style="width: 32px;
                                height: 32px;">
                                </div> -->
                            <i class="fa fa-paperclip" aria-hidden="true" id="icon_cir" (click)="paperClip()"></i>
                        </div>
                        
                    </div>
                    <div class="scripture-options">
                        <div class="scripture-options-item btn-group" dropdown>
                            <button id="scripture-more-options" dropdownToggle type="button" class="option-menu-btn"
                                aria-controls="dropdown-basic" id="colw" >
                                <!-- <i class="fa fa-ellipsis-v"></i> -->
                                <fa name="ellipsis-v" style="color: white" fixed='true'></fa>
                            </button>
                            <div id="scripture-more-options-menu" *dropdownMenu
                                class="dropdown-menu dropdown-with-check" role="menu"
                                aria-labelledby="scripture-more-options">
                                <a href="javascript:void(0)" class="dropdown-item"
                                    [ngClass]="{'selected': isLareevaarShow}"
                                    (click)="onSelectLareevaarScriptureOption()">
                                    <i class="fa fa-fw fa-check"></i>
                                    Gurmukhi - Lareevaar
                                </a>
                                <a href="javascript:void(0)" class="dropdown-item" [ngClass]="{'selected': isRomanShow}"
                                    (click)="onSelectRomanScriptureOption()">
                                    <i class="fa fa-fw fa-check"></i>
                                    Roman Script
                                </a>
                            </div>
                        </div>
                    </div>
                    <!-- <ul class="pagination">
            <li class="page-label">Shabad</li>
            <li class="page-item" *ngFor="let page of shabadPages"
                [ngClass]="{'active': page === currentShabadPage}">
                <a class="page-link" [routerLink]="['/shabad', pageId, page]">{{page}}</a>
            </li>
            </ul> -->
                </nav>
            </div>
            <div class="column-right d-flex justify-content-end align-items-center" style="padding-right: 10px; font-size: 21px!important;" >
                    Translation
                <!-- <div class="btn-group author-dropdown" dropdown>
                    <button id="choose-translation" dropdownToggle type="button" class="dropdown-toggle"
